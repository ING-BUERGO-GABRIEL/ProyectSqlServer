                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT a.address_id,
       a.street_number,
       a.street_name,
       a.city,
       a.country_id,
       c.country_name,
       ca.status_id AS address_status_id,
       ast.address_status
FROM [dbo].[address] AS a
LEFT JOIN [dbo].[country] AS c ON a.country_id = c.country_id
LEFT JOIN [dbo].[customer_address] AS ca ON a.address_id = ca.address_id
LEFT JOIN [dbo].[address_status] AS ast ON ca.status_id = ast.status_id
WHERE a.[rowversion] > CONVERT(ROWVERSION, ?) AND a.[rowversion] <= CONVERT(ROWVERSION, ?);</property>
