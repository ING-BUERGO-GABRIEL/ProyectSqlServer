                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT b.book_id,
       b.title,
       b.isbn13,
       b.language_id,
       bl.language_code,
       bl.language_name,
       b.num_pages AS number_of_pages,
       b.publication_date,
       b.publisher_id,
       p.publisher_name
FROM [dbo].[book] AS b
LEFT JOIN [dbo].[book_language] AS bl ON b.language_id = bl.language_id
LEFT JOIN [dbo].[publisher] AS p ON b.publisher_id = p.publisher_id
WHERE b.[rowversion] > CONVERT(ROWVERSION, ?) AND b.[rowversion] <= CONVERT(ROWVERSION, ?);</property>
